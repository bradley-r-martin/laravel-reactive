{
    "name": "sihq/laravel-reactive",
    "description": "Laravel Reactive - Easy communication with react.",
    "keywords": [
        "sihq",
        "laravel-reactive"
    ],
    "homepage": "https://github.com/sihq/laravel-reactive",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bradley Mar",
            "email": "bradley.r.martin@me.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sihq\\Reactive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sihq\\Reactive\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sihq\\Reactive\\ReactiveServiceProvider"
            ],
            "aliases": {
                "Reactive": "Sihq\\Reactive\\ReactiveFacade"
            }
        }
    }
}
